<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.packetboard.packetboard.HelloController">
   <children>
      <BorderPane prefHeight="800.0" prefWidth="1213.0" AnchorPane.rightAnchor="0.0">
         <center>
            <VBox alignment="CENTER" fillWidth="false" prefHeight="423.0" prefWidth="646.0" BorderPane.alignment="CENTER">
               <children>
                  <Label text="PacketBoard">
                     <font>
                        <Font size="29.0" />
                     </font>
                  </Label>
                  <HBox prefHeight="26.0" prefWidth="110.0">
                     <children>
                        <Button fx:id="loadButton" mnemonicParsing="false" onAction="#onImportBtnClick" prefHeight="25.0" prefWidth="105.0" text="Load pcap" textAlignment="CENTER" />
                     </children>
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                  </HBox>
                  <HBox prefHeight="27.0" prefWidth="120.0">
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                     <children>
                        <CheckBox fx:id="sniBox" mnemonicParsing="false" prefHeight="17.0" prefWidth="139.0" text="Domain Lookup" />
                     </children>
                  </HBox>
                  <HBox prefHeight="27.0" prefWidth="120.0">
                     <children>
                        <CheckBox fx:id="rdnsBox" mnemonicParsing="false" prefHeight="17.0" prefWidth="134.0" text="rDNS Lookup" />
                     </children>
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                  </HBox>
               </children>
               <BorderPane.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </BorderPane.margin>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </VBox>
         </center>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </BorderPane>
   </children>
</AnchorPane>
